{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier/prettier",
        "plugin:node/recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-multiple-empty-lines": "off",
        "func-names": "off",
        "camelcase": "off",
        "object-curly-newline": "off"
    }
}
